<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>SQL Stress Menú Pricipal</title>
	@Styles.Render("~/Vendors/css")
	@Styles.Render("~/Shared/css")
	@RenderSection("styles", required: false)
</head>
<body>
	@{ Html.RenderPartial("~/Views/Shared/NavBar/_NavPaginaPrincipal.cshtml");}
	@RenderBody()
	<div class="form-group form-group-sm col-sm-6">
		<div class="row">
			<label for="ReporteBase" class="col-sm-6 col-form-label" style="font-size: 25px; padding:10px;"> Ingrese los datos requeridos </label>
		</div>
	</div>
	<br />
	<form class="form-horizontal">
		<div class="container">
			<div class="row">
				<div class="form-group form-group-sm col-sm-7">
					<div class="row">
						<label for="NombreBase" class="col-sm-6 col-form-label">Nombre de la base de datos</label>
						<div class="col-sm-4">
							<select style="width:246px; font-size:20px">
								
							</select>
						</div>
					</div>
				</div>
				<div class="form-group form-group-sm col-sm-7">
					<div class="row">
						<label for="NombreTabla" class="col-sm-6 col-form-label">Nombre de la tabla</label>
						<div class="col-sm-5">
							<input type="text" class="form-control" id="NomTabla" name="NomTabla">
						</div>
					</div>
				</div>
				<div class="form-group form-group-sm col-sm-7">
					<div class="row">
						<label for="CantConsul" class="col-sm-6 col-form-label">Cantidad de consultas requeridos</label>
						<div class="col-sm-5">
							<input type="text" class="form-control" id="CantConsul" name="CantConsul">
						</div>
					</div>
				</div>

				<div class="form-group form-group-sm col-sm-6">
					
						<button type="submit" class="btn btn-primary">Realizar Proceso</button>
						<button type="submit" class="btn btn-primary">Salir</button>
				</div>
			</div>
		</div>
	</form>
	@{ Html.RenderPartial("~/Views/Shared/Footer/_FooterPrincipal.cshtml");}
</body>
</html>
